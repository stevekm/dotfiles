"\e[A": history-search-backward
"\e[B": history-search-forward


# mappings for Ctrl-left-arrow and Ctrl-right-arrow for word moving
"\e[1;5C": forward-word
"\e[1;5D": backward-word
"\e[5C": forward-word
"\e[5D": backward-word
"\e\e[C": forward-word
"\e\e[D": backward-word


# NOTE: zsh does not use .inputrc file, it uses .zshrc instead
# see the settings already in use for the zsh_extras file
